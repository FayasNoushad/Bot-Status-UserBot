{
    "name": "Bot-Status-UserBot",
    "description": "A user bot for stat telegram bots online status",
    "keywords": ["telegram", "bot status", "user bot"],
    "env": {
        "API_ID": {
            "description": "Your APP_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        },
        "BOT_OWNER": {
            "description": "Your username without '@'"
        },
        "BOTS": {
            "description": "Your all bots username without '@' with a space 1 to another",
            "required": false
        },
        "UPDATE_CHANNEL": {
            "description": "Your channel id"
        },
        "MESSAGE_ID": {
            "description": "Message id of your channel bot status message"
        },
        "TIME_LIMIT": {
            "description": "Time limit of editing (in minutes)",
            "required": false
        },
        "SLEEP_TIME": {
            "description": "Sleep time for checking bot",
            "required": false
        },
        "SESSION_STRING": {
            "description": "Your pyrogram session string"
        },
        "HEADING": {
            "description": "Status message heading in html or markdown format"
        },
        "ATTACH_LINK": {
            "description": "Attach link for attaching links in status message",
            "required": false
        }
    }
}
